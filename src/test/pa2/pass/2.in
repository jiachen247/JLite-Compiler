        d.z = "1";

        return ;
    }

    Void testControlFlows() {
        Int c;
        Bool b;
        Int x;
        Dummy d;

        if (false) {
            b = 1 == 2;
        } else {
            c = 1;
        }

        // return type match
        if (true) {
            d.retInt();
        } else {
            d.retInt();
        }

        c = 0;

        while (c < 10) {
            c = c + 1;
        }
        b = true;
        while (b) {
            x = x + 1;
        }

        this.x = test();
        x = test();
        x = d.retInt();

    }

    Int test() {
        return 1;
    }

    Int testInvoke() {
        Int a;
        Dummy d;

        d = new Dummy();
        a = test();
        a = d.test(2);
       return a;
    }
}

class TestExpressions {
    Void test() {
        Int x;
        String s;
        Bool b;
        Dummy d;

        // New Expression
        d = new Dummy();

        // Unary Expressions
        x = -1;
        b = !true;
        b = !(1 > 2);

        // Boolean Expressions
        x = 1 - 2;
        x = 1 + 2;
        x = 1 * 2;
        x = 1 / 2;
        b = 1 > 2;
        b = 1 < 2;
        b = 1 == 1;
        s = s + "123";

        // Call Expression
        x = d.test(1);

    }
}