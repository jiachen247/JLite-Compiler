class Main {
    Void main() {
        Factorial factorial;
        Int answer;

        factorial = new Factorial();
        answer = factorial.calculate(5);
        println(answer);
    }
}

class Factorial {
    Int calculate(Int n) {
        if (n == 1) {
            return 1;
        } else {
            return n * calculate(n - 1);
        }
    }
}
