/* OOP Test */

class Main {
    Void main() {
        Cat cat;
        Dog dog;

        cat = new Cat();
        cat.build(1);
        cat.setName("kitty cat");

        dog = new Dog();
        dog.build(2);
        dog.setName("doggy");

        cat.print();
        dog.print();
    }
}

class Animal {
    String type;
    String name;
    String sound;
    Int age;

    Void build(String type, String sound, Int age) {
        this.type = type;
        this.sound = sound;
        this.age = age;
    }

    Void setName(String name) {
        this.name = name;
    }

    Void print() {
        println("I am a " + type + " and my name is " + name + ". I am ");
        println(age);
        println(" years old. I go ");
        println(sound + " ");
        println(sound + " ");
        println(sound + "!!!\n");
    }

}


class Dog {
    Animal animal;

    Void build(Int age) {
        this.animal = new Animal();
        animal.build("Dog", "bark", age);
    }

    Void setName(String name) {
        this.animal.name = name;
    }

    Void print() {
        this.animal.print();
    }
}

class Cat {
    Animal animal;

    Void build(Int age) {
        this.animal = new Animal();
        println("whyutt");
        animal.build("Cat", "meow", age);
    }

    Void setName(String name) {
        this.animal.name = name;
    }

    Void print() {
        this.animal.print();
    }
}