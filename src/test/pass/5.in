// <ClassDecl>
class Class1 {
    // <VarDecl>
    Int a;
    Bool b;
    String c;
    NewClass newClass;

    // <MdDecl>
    Void method1(Int arg1, Void arg2) {
        // <MdBody>

        // <VarDecl>
        Int a;
        Bool b;
        String c;
        NewClass newclass;

        // <Stmt+>
        if (1 == 1) {
            return ;
        } else {
            return ;
        }

        // <Atom>
        id = atom.id;
        id = atom(1, 2, 3);
        id = this;
        id = id;
        id = new NewClass();
        id = -(0);
        id = null;

        return ;
    }

